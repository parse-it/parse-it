{
    "name": "@parse-it/database",
    "version": "0.1.1",
    "description": "A simple query builder for TypeScript",
    "main": "dist/index.js",
    "module": "dist/index.mjs",
    "types": "dist/index.d.ts",
    "type": "module",
    "files": [
        "dist"
    ],
    "scripts": {
        "build": "tsup src/index.ts --format cjs,esm --dts",
