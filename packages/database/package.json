        "directory": "packages/database"
    },
    "bugs": {
        "url": "https://github.com/parse-it/parse-it/issues"
    },
    "homepage": "https://github.com/parse-it/parse-it#readme",
    "devDependencies": {
        "tsup": "^8.3.5",
        "typescript": "^5.7.2",
        "vitest": "^2.1.8"
    },
    "dependencies": {
        "peggy": "^4.2.0"
